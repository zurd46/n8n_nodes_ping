{
  "name": "@zurdai/n8n-nodes-server-check",
  "version": "1.0.1",
  "description": "n8n nodes for server monitoring - check connectivity via HTTP, TCP Port and DNS",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "server-check",
    "http-check",
    "tcp-port",
    "dns",
    "network",
    "monitoring",
    "connectivity",
    "uptime"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Zurdai"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes --ext .ts --fix",
    "lintcheck": "eslint nodes --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Ping/Ping.node.js",
      "dist/nodes/Ping/PingTrigger.node.js"
    ],
    "credentials": []
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
